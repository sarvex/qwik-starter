{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=18.16.0"
  },
  "private": true,
  "scripts": {
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.storybook": "storybook build",
    "build.types": "tsc --incremental --noEmit",
    "build": "qwik build",
    "cypress.open": "cypress open --component",
    "cypress.run": "cypress run --component",
    "deploy": "vercel deploy",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev": "vite --mode ssr",
    "fmt.fix": "prettier --write .",
    "fmt": "prettier --check .",
    "lint.fix": "eslint --cache --fix . --report-unused-disable-directives",
    "lint": "eslint . --cache --report-unused-disable-directives",
    "ls.fix": "ls-lint",
    "ls": "ls-lint",
    "markdown.fix": "markdownlint-cli2-fix",
    "markdown": "markdownlint-cli2",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "preview": "qwik build preview && vite preview --open",
    "prisma.generate": "prisma generate",
    "prisma.migrate:prod": "prisma migrate deploy",
    "prisma.migrate": "prisma migrate dev",
    "qwik": "qwik",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "style.fix": "stylelint .",
    "style": "stylelint --fix .",
    "test.e2e": "playwright test",
    "test.unit.ui": "vitest --ui components",
    "test.unit": "vitest components"
  },
  "devDependencies": {
    "@builder.io/partytown": "0.8.0",
    "@builder.io/qwik": "1.2.6",
    "@builder.io/qwik-city": "1.2.6",
    "@builder.io/sdk-qwik": "0.4.5",
    "@ls-lint/ls-lint": "2.0.1",
    "@playwright/test": "1.36.2",
    "@prisma/client": "5.0.0",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/blocks": "7.1.1",
    "@storybook/builder-vite": "7.1.1",
    "@storybook/html": "7.1.1",
    "@storybook/html-vite": "7.1.1",
    "@types/eslint": "8.44.1",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vitest/ui": "0.33.0",
    "autoprefixer": "10.4.14",
    "cypress": "12.17.2",
    "cypress-ct-qwik": "0.3.0",
    "eslint": "8.46.0",
    "eslint-plugin-qwik": "1.2.6",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prisma": "5.0.0",
    "rome": "12.1.3",
    "storybook": "7.1.1",
    "storybook-framework-qwik": "0.2.3",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "10.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vercel": "31.2.0",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
}
